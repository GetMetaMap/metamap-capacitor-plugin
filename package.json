{
  "name": "metamap-capacitor-plugin",
  "version": "4.8.4",
  "description": "Capacitor plugin for MetaMap SDK",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Avo Sukiasyan",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/cli": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/ios": "^7.0.0",
    "@ionic/prettier-config": "^2.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "prettier": "^2.8.8",
    "prettier-plugin-java": "^1.6.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "swiftlint": "^1.0.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@capacitor/core": "^7.0.0"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "MetamapCapacitorPlugin.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "MetaMap",
    "identity-verification"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/GetMetaMap/mati-capacitor-plugin"
  },
  "bugs": {
    "url": "https://github.com/GetMetaMap/mati-capacitor-plugin/issues"
  },
  "homepage": "https://github.com/GetMetaMap/mati-capacitor-plugin#readme"
}